.EXPORT_ALL_VARIABLES:

MAKE = @MAKE@
RM = @RM@ -f

BINARY_PATH = ..

.PHONY: all clean depend distclean install uninstall
all:
	${MAKE} -C src all

depend:
	${MAKE} -C src depend

install:
	${MAKE} -C src install

uninstall:
	${MAKE} -C src uninstall

clean:
	${MAKE} -C src clean

distclean: clean
	${MAKE} -C src distclean
	${RM} config.cache config.log config.status Makefile
