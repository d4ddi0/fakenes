setstdout "${STDOUT_LOG}"
setstderr "${STDERR_LOG}"

@!call "${CC}" -E "${BUILD_DIR}/zlib.c ${CPPFLAGS}"

do ifnret 0
   setstdout ''
   setstderr ''

   echo 'you\'re missing zlib'
   echo 'continuing on without it...'

   uninvoke 0
done

setstdout ''
setstderr ''

# --

SRCFILES += 'unzip.c '

# --

CFLAGS += ' -DUSE_ZLIB'

# --

LDFLAGS += ' -lz'
